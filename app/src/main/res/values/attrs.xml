<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ActionBarView">
        <attr name="field_action_bar_type" format="enum">
            <enum name="type_search" value="0"/>
            <enum name="type_logo_with_back" value="1"/>
            <enum name="type_logo_with_back_transparent" value="2"/>
            <enum name="type_logo_without_back" value="3"/>
            <enum name="type_text_with_back" value="4"/>
        </attr>
        <attr name="field_action_bar_title" format="string"/>
        <attr name="field_action_bar_hint" format="string"/>
        <attr name="field_action_bar_theme" format="enum">
            <enum name="light" value="0"/>
            <enum name="dark" value="1"/>
        </attr>
    </declare-styleable>
</resources>